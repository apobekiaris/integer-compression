<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.0</TargetFramework>
    <AssemblyName>InvertedTomato.IntegerCompression</AssemblyName>
    <RootNamespace>InvertedTomato</RootNamespace>
    <PackageLicenseUrl>https://github.com/invertedtomato/integer-compression/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/invertedtomato/integer-compression&lt;</PackageProjectUrl>
    <RepositoryUrl>https://github.com/invertedtomato/integer-compression&lt;</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Compression VLQ Elias Omega Gamma Delta Fibonacci Integer Numbers</PackageTags>
    <Version>1.1.0</Version>
    <Description>Library for compressing integers in the minimal number of bits. Includes VLQ, Elias Omega, Elias Gamma, Elias Delta, Fibonacci and others.</Description>
    <Copyright>Inverted Tomato</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="InvertedTomato.Buffers" Version="1.0.6" />
  </ItemGroup>

</Project>